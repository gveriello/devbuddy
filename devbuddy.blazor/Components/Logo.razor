<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 80" width="300" height="180" style="background-color: transparent;">
    <!-- Geometrical Abstract Code Symbol -->
    <g class="icon">
        <!-- Code blocks representing development -->
        <rect x="15" y="20" width="16" height="16" rx="3" fill="#4f46e5" />
        <rect x="15" y="44" width="16" height="16" rx="3" fill="#818cf8" />
        <rect x="39" y="20" width="16" height="16" rx="3" fill="#818cf8" />
        <rect x="39" y="44" width="16" height="16" rx="3" fill="#4f46e5" />

        <!-- Code lines connecting the blocks -->
        <line x1="31" y1="28" x2="39" y2="28" stroke="#a5b4fc" stroke-width="2" />
        <line x1="31" y1="52" x2="39" y2="52" stroke="#a5b4fc" stroke-width="2" />
        <line x1="23" y1="36" x2="23" y2="44" stroke="#a5b4fc" stroke-width="2" />
        <line x1="47" y1="36" x2="47" y2="44" stroke="#a5b4fc" stroke-width="2" />

        <!-- Code brackets -->
        <path d="M10,30 L5,40 L10,50" stroke="#c7d2fe" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" />
        <path d="M60,30 L65,40 L60,50" stroke="#c7d2fe" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" />
    </g>

    <!-- Text -->
    <g class="text">
        <text x="80" y="45" font-family="Arial, sans-serif" font-weight="700" font-size="22" letter-spacing="0.5">
            <tspan fill="#4338CA">Dev</tspan><tspan fill="#818CF8">Buddy</tspan>
        </text>
        <text x="80" y="60" font-family="Arial, sans-serif" font-size="10" fill="#6B7280">Developer's Toolkit</text>
    </g>
</svg>